<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
        <servlet-name>TestCookieServlet</servlet-name>
        <servlet-class>com.zledu.listener.TestCookieServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>TestCookieServlet</servlet-name>
        <url-pattern>/TestCookieServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>GetCookieServlet</servlet-name>
        <servlet-class>com.zledu.listener.GetCookieServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>GetCookieServlet</servlet-name>
        <url-pattern>/GetCookieServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>RememberServlet</servlet-name>
        <servlet-class>com.zledu.listener.RememberServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>RememberServlet</servlet-name>
        <url-pattern>/RememberServlet</url-pattern>
    </servlet-mapping>
</web-app>