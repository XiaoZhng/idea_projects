<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <listener>
        <listener-class>com.zledu.listener.TestListener</listener-class>
    </listener>
    <servlet>
        <servlet-name>TestAjaxServlet</servlet-name>
        <servlet-class>com.zledu.servlet.TestAjaxServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>TestAjaxServlet</servlet-name>
        <url-pattern>/TestAjaxServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>TestJsonServlet</servlet-name>
        <servlet-class>com.zledu.servlet.TestJsonServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>TestJsonServlet</servlet-name>
        <url-pattern>/TestJsonServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>UpServlet</servlet-name>
        <servlet-class>com.zledu.servlet.UpServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>UpServlet</servlet-name>
        <url-pattern>/UpServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>DownServlet</servlet-name>
        <servlet-class>com.zledu.servlet.DownServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>DownServlet</servlet-name>
        <url-pattern>/DownServlet</url-pattern>
    </servlet-mapping>
</web-app>